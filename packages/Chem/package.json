{
  "name": "Chem",
  "version": "0.8.18",
  "fullName": "Cheminformatics support",
  "description": "Cheminformatics support: \n\n* RDKit-based rendering \n* Molecule sketching \n*",
  "sources": [
    "css/chem.css",
    "src/RDKit_minimal_2021.03_03.js",
    "src/rdkit_worker_proxy.js",
    "src/rdkit_substruct_library.js",
    "src/rdkit_parallel.js",
    "src/rdkit_cell_renderer.js",
    "src/rdkit_substructure_filter.js",
    "src/chem_column_property_panel.js",
    "src/chem_searches.js"
  ],
  "properties": [
    {
      "name": "Renderer",
      "propertyType": "string",
      "choices": [
        "OCL",
        "RDKit"
      ],
      "defaultValue": "RDKit"
    },
    {
      "name": "Searches",
      "propertyType": "string",
      "choices": [
        "RDKit Server",
        "RDKit Client"
      ],
      "defaultValue": "RDKit Server"
    }
  ],
  "dependencies": {
    "datagrok-tools": "^4.0.15"
  }
}
