import numpy as np
import pyphysio as ph

sig = ph.EvenlySignal(
    values=np.array(input_signals.iloc[:,0]),
    sampling_freq=sampling_frequency
)

i = len(parameters) - 1
sig = sig.resample(
    fout=parameters['fout'][i],
    kind=parameters['kind'][i]
)

df = pd.DataFrame({
    'time': range(len(sig)),
    'sig': sig
})