#input: double fp
#input: double fs
#output: dataframe df

import pyphysio as ph

sig = ph.EvenlySignal(values=input_signals.iloc[:,0], sampling_freq=sampling_frequency)

sig = ph.FIRFilter(fp=[fp], fs=[fs])(sig)

df = pd.DataFrame({
    'time': range(len(sig)),
    'sig': sig
})