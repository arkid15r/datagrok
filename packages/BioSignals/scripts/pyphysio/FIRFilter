import numpy as np
import pyphysio as ph

sig = ph.EvenlySignal(values=np.array(input_signals.iloc[:,0]), sampling_freq=sampling_frequency)

sig = ph.FIRFilter(fp=[fp], fs=[fs])(sig)

df = pd.DataFrame({
    'time': range(len(sig)),
    'sig': sig
})